{
  "name": "smart-error-lens",
  "version": "0.1.0",
  "description": "AI-powered error analysis for TypeScript/Node.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "error-handling",
    "typescript",
    "decorator",
    "debugging",
    "ai",
    "openai",
    "error-analysis"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.51.0",
    "@google-ai/generativelanguage": "^3.2.0",
    "@huggingface/inference": "^3.13.1",
    "google-auth-library": "^10.0.0-rc.2",
    "openai": "^4.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xykine-ademola-hammed/smart-error-lens"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
